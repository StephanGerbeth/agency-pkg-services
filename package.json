{
  "name": "agency-pkg-services",
  "version": "0.0.1",
  "description": "agency - frontend package services",
  "keywords": [
    "boilerplate",
    "package",
    "services"
  ],
  "homepage": "https://github.com/StephanGerbeth/agency-pkg-services#readme",
  "bugs": {
    "url": "https://github.com/StephanGerbeth/agency-pkg-services/issues"
  },
  "license": "MIT",
  "author": "Stephan Gerbeth <stephan.gerbeth@gmail.com>",
  "contributors": [{
    "name": "Stephan Gerbeth",
    "email": "stephan.gerbeth@grabarzundpartner.de"
  }, {
    "name": "Thorn Walli",
    "email": "lammpee@gmail.com; thorn.walli@grabarzundpartner.de"
  }],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephanGerbeth/agency-pkg-services.git"
  },
  "scripts": {
    "install": "node ./install.js",
    "heroku": "node node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/local.json --configTasks=./env/config/tasks.json --env=package-production --server=agency-server",
    "dev": "node node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/local.json --configTasks=env/config/tasks.json --env=package-development --server=agency-server",
    "prod": "node node_modules/gulp/bin/gulp run --serverConfig=node_modules/agency-boilerplate/env/config/local.json --configTasks=env/config/tasks.json --env=production --server=agency-server"
  },
  "dependencies": {
    "historyjs": "^1.8.0-b2",
    "agency-pkg-base": "git+https://git@github.com/StephanGerbeth/agency-pkg-base.git#feature/flat-files"
  },
  "devDependencies": {
    "agency-boilerplate": "git+https://git@github.com/StephanGerbeth/agency-boilerplate.git#feature/flat-files"
  }
}
